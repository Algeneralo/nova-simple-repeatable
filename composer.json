{
  "name": "optimistdigital/nova-simple-repeatable",
  "description": "A Laravel Nova simple repeatable rows field.",
  "keywords": [
    "laravel",
    "nova",
    "repeatable",
    "flexible",
    "rows",
    "keyvalue",
    "field"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "laravel/nova": "~4.0",
    "optimistdigital/nova-translations-loader": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "OptimistDigital\\NovaSimpleRepeatable\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "OptimistDigital\\NovaSimpleRepeatable\\SimpleRepeatableServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
